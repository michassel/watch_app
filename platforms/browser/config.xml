<?xml version='1.0' encoding='utf-8'?>
<widget id="com.michassel.watchapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>WatchApp</name>
    <description>
        First mobile app on android with PhoneGap and cordova framework.
    </description>
    <author email="michassel@gmail.com" href="http://kreatortechnologii.pl">
        michassel
    </author>
    <content src="index.html" />
    <access launch-external="yes" origin="*" />
    <access launch-external="yes" origin="mailto:*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" launch-external="yes" />
    <allow-intent href="geo:*" />
    <preference name="Fullscreen" value="true" />
</widget>
